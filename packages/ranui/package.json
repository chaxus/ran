    "./message": {
      "types": "./dist/components/message/index.d.ts",
      "import": "./dist/components/message/index.js",
      "require": "./dist/umd/index.umd.cjs"
    },
    "./preview": {
      "types": "./dist/components/preview/index.d.ts",
