    },
    "./type": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./typings": {
      "types": "./dist/typings.d.ts",
      "import": "./dist/typings.d.ts",
      "require": "./dist/index.js"
    },
    "./dist/*": {
      "types": "./dist/*",
      "import": "./dist/*",
      "require": "./dist/*"
    }
