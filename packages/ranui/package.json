      "require": "./dist/umd/index.umd.cjs"
    },
    "./option": {
      "types": "./dist/components/option/index.d.ts",
      "import": "./dist/components/option/index.js",
      "require": "./dist/umd/index.umd.cjs"
    },
    "./progress": {
      "types": "./dist/components/progress/index.d.ts",
