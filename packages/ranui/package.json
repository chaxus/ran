    "@types/react": "^19.1.0",
    "@types/tinycolor2": "^1.4.6",
    "imagemin-svgo": "^11.0.1",
    "less": "^4.2.2",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "svgo": "^3.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.3.4"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "exceljs": "^4.4.0",
